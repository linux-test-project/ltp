#!/bin/sh

# undivert.test is part of the GNU m4 testsuite

#. ${srcdir}/defs

cat ${srcdir}/undivert.m4 >in

cat <<\EOF >ok

# undiverted file.
This is to be undiverted soon.

# included file.
This is to be UNDIVERTED soon.

EOF

M4PATH=$srcdir:$srcdir/../tests $M4 -d in >out 2>err
sed -e "s, $M4:, m4:," err >sederr && mv sederr err
$CMP -s out ok
